<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net9.0-windows</TargetFramework>
		<UseWPF>true</UseWPF>
		<UseWindowsForms>true</UseWindowsForms>
		<PlatformTarget>AnyCPU</PlatformTarget>
		<Version>1.0.0</Version>
	</PropertyGroup>
	<ItemGroup>
		<Content Include="TestWeb\default.html">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<Content Include="TestWeb\index.html">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="CefSharp.Wpf.NETCore" />
		<PackageReference Include="CommunityToolkit.Mvvm" />
		<PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Update="chromiumembeddedframework.runtime.win-arm64" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Update="chromiumembeddedframework.runtime.win-x64" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Update="chromiumembeddedframework.runtime.win-x86" />
	</ItemGroup>
</Project>