<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Background_Normal"
                     Color="#0E77FF" />
    <SolidColorBrush x:Key="Background_MouseOver"
                     Color="#FF12AEE9" />
    <SolidColorBrush x:Key="Background_Disabled"
                     Color="#FFF4F4F4" />

    <SolidColorBrush  x:Key="BorderBrush_Normal"
                      Color="#5EA1F7" />
    <SolidColorBrush x:Key="BorderBrush_MouseOver"
                     Color="#FF108BAE" />
    <SolidColorBrush x:Key="BorderBrush_Disabled"
                     Color="#FFADB2B5" />
    <SolidColorBrush x:Key="BorderBackground"
                     Color="#122A5E" />

    <FontFamily x:Key="YaHeiFont">Fonts/#msyh</FontFamily>
    
</ResourceDictionary>